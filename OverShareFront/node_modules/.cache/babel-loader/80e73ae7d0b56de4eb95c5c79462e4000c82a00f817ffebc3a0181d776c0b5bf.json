{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"ConcernList\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      creatorList: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  methods: {\n    //请求创作者数据\n    load() {\n      this.$request.get(\"/concern/selectConcernList/\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userId: this.user.id\n        }\n      }).then(res => {\n        this.creatorList = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    cancelConcern(creatorId) {\n      this.$request.delete(\"/concern/delete/\" + this.user.id + \"/\" + creatorId).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"取消关注成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    homePage(userId) {\n      this.$router.push('/front/HomePage?userId=' + userId);\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","creatorList","pageNum","pageSize","total","mounted","load","methods","$request","get","params","userId","id","then","res","list","cancelConcern","creatorId","delete","code","$message","success","error","msg","handleCurrentChange","homePage","$router","push"],"sources":["src/components/ConcernList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"card\"\r\n         style=\"padding: 10px; display: flex; justify-content: space-between; margin-bottom: 10px\"\r\n         v-for=\"item in creatorList\" :key=\"item.id\">\r\n      <!-- 头像与用户名组合-->\r\n      <div style=\"display: flex; align-items: center; margin-right: 15px;\" @click=\"homePage(item.creatorId)\">\r\n        <el-avatar :size=\"70\" :src=\"item.avatar\"></el-avatar>\r\n        <span style=\"font-weight: bold; font-size: 30px; margin-left: 10px;\">{{ item.userName }}</span>\r\n      </div>\r\n      <!-- 关注/取消关注按钮 -->\r\n      <div style=\"margin-left: auto; display: flex; align-items: center;\">\r\n        <el-button round type=\"warning\" @click=\"cancelConcern(item.creatorId)\" size=\"medium\">取消关注</el-button>\r\n      </div>\r\n    </div>\r\n    <div v-if=\"total === 0\" style=\"padding: 20px;text-align: center; font-size: 16px; color: #666\">暂无数据</div>\r\n    <!--分页-->\r\n    <div class=\"pagination\" style=\"margin-top: 10px\" v-if=\"total\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"ConcernList\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      creatorList: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    //请求创作者数据\r\n    load() {\r\n      this.$request.get(\"/concern/selectConcernList/\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id\r\n        }\r\n      }).then(res => {\r\n        this.creatorList = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    cancelConcern(creatorId) {\r\n      this.$request.delete(\"/concern/delete/\" + this.user.id + \"/\" + creatorId).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"取消关注成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        