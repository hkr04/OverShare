{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"MyWelfare\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      myWelfare: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      content: '',\n      title: '',\n      fromVisible1: false\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  methods: {\n    //请求我的福利数据\n    load() {\n      this.$request.get(\"/myWelfare/selectByUserId/\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userId: this.user.id\n        }\n      }).then(res => {\n        this.myWelfare = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    detail(content, title) {\n      this.content = content;\n      this.title = title;\n      this.fromVisible1 = true;\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    homePage(userId) {\n      this.$router.push('/front/HomePage?userId=' + userId);\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","myWelfare","pageNum","pageSize","total","content","title","fromVisible1","mounted","load","methods","$request","get","params","userId","id","then","res","list","detail","handleCurrentChange","homePage","$router","push"],"sources":["src/components/MyWelfare.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"card\"\r\n         style=\"padding: 10px; display: flex; justify-content: space-between; margin-bottom: 10px\"\r\n         v-for=\"item in myWelfare\" :key=\"item.id\">\r\n      <!-- 头像与用户名组合-->\r\n      <div style=\"position: relative; margin-right: 15px; text-align: center\" @click=\"homePage(item.dbUserId)\">\r\n        <el-avatar :size=\"50\" :src=\"item.avatar\"></el-avatar>\r\n        <div style=\"font-size: 16px; color: #999999\">{{ item.username }}</div>\r\n      </div>\r\n      <!--标题-->\r\n      <div style=\"margin-right: 15px; flex: 1; align-items: center; max-width: 600px; overflow: hidden\">\r\n        <div class=\"line1\" style=\"font-size: 20px; font-weight: bold; margin-bottom: 22px;margin-top: 10px\">\r\n          {{ item.title }}\r\n        </div>\r\n        <div style=\"font-size: 12px; color: #999999\">{{ item.time }}</div>\r\n      </div>\r\n      <!-- 查看详情按钮 -->\r\n      <div style=\"margin-left: auto; display: flex; align-items: center;\">\r\n        <el-button round type=\"warning\" @click=\"detail(item.content,item.title)\" size=\"medium\">查看详情</el-button>\r\n      </div>\r\n    </div>\r\n    <div v-if=\"total === 0\" style=\"padding: 20px;text-align: center; font-size: 16px; color: #666\">暂无数据</div>\r\n\r\n    <!--分页-->\r\n    <div class=\"pagination\" style=\"margin-top: 10px\" v-if=\"total\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog title=\"查看详情\" :visible.sync=\"fromVisible1\" width=\"60%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div style=\"font-weight: bold; font-size: 28px\">\r\n        {{ title }}\r\n      </div>\r\n      <div class=\"w-e-text\">\r\n        <div v-html=\"content\"></div>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible1 = false\">关 闭</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"MyWelfare\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      myWelfare: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      content: '',\r\n      title: '',\r\n      fromVisible1: false,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    //请求我的福利数据\r\n    load() {\r\n      this.$request.get(\"/myWelfare/selectByUserId/\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id\r\n        }\r\n      }).then(res => {\r\n        this.myWelfare = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    detail(content, title) {\r\n      this.content = content\r\n      this.title = title\r\n      this.fromVisible1 = true\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    homePage(userId){\r\n      this.$router.push('/front/HomePage?userId=' + userId)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.line1 {\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n</style>"],"mappings":";AAkDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,OAAA;MACAC,KAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACA;IACAD,KAAA;MACA,KAAAE,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAX,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAW,MAAA,OAAAlB,IAAA,CAAAmB;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA,KAAAhB,SAAA,GAAAgB,GAAA,CAAAtB,IAAA,EAAAuB,IAAA;QACA,KAAAd,KAAA,GAAAa,GAAA,CAAAtB,IAAA,EAAAS,KAAA;MACA;IACA;IACAe,OAAAd,OAAA,EAAAC,KAAA;MACA,KAAAD,OAAA,GAAAA,OAAA;MACA,KAAAC,KAAA,GAAAA,KAAA;MACA,KAAAC,YAAA;IACA;IACAa,oBAAAlB,OAAA;MACA,KAAAO,IAAA,CAAAP,OAAA;IACA;IACAmB,SAAAP,MAAA;MACA,KAAAQ,OAAA,CAAAC,IAAA,6BAAAT,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}