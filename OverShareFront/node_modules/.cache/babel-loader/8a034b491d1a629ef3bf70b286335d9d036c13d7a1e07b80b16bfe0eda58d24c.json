{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"ConcernList\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      creatorList: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  methods: {\n    //请求创作者数据\n    load() {\n      this.$request.get(\"/concern/selectConcernList/\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userId: this.user.id\n        }\n      }).then(res => {\n        this.creatorList = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    cancelConcern(creatorId) {\n      this.$request.delete(\"/concern/delete/\" + this.user.id + \"/\" + creatorId).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"取消关注成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    homePage(userId) {\n      this.$router.push('/front/HomePage?userId=' + userId);\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","creatorList","pageNum","pageSize","total","mounted","load","methods","$request","get","params","userId","id","then","res","list","cancelConcern","creatorId","delete","code","$message","success","error","msg","handleCurrentChange","homePage","$router","push"],"sources":["src/components/ConcernList.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"card\"\n         style=\"padding: 10px; display: flex; justify-content: space-between; margin-bottom: 10px\"\n         v-for=\"item in creatorList\" :key=\"item.id\">\n      <!-- 头像与用户名组合-->\n      <div style=\"display: flex; align-items: center; margin-right: 15px;\" @click=\"homePage(item.creatorId)\">\n        <el-avatar :size=\"70\" :src=\"item.avatar\"></el-avatar>\n        <span style=\"font-weight: bold; font-size: 30px; margin-left: 10px;\">{{ item.userName }}</span>\n      </div>\n      <!-- 关注/取消关注按钮 -->\n      <div style=\"margin-left: auto; display: flex; align-items: center;\">\n        <el-button round type=\"warning\" @click=\"cancelConcern(item.creatorId)\" size=\"medium\">取消关注</el-button>\n      </div>\n    </div>\n    <div v-if=\"total === 0\" style=\"padding: 20px;text-align: center; font-size: 16px; color: #666\">暂无数据</div>\n    <!--分页-->\n    <div class=\"pagination\" style=\"margin-top: 10px\" v-if=\"total\">\n      <el-pagination\n          background\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, prev, pager, next\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"ConcernList\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      creatorList: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n    }\n  },\n  mounted() {\n    this.load()\n  },\n  methods: {\n    //请求创作者数据\n    load() {\n      this.$request.get(\"/concern/selectConcernList/\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userId: this.user.id\n        }\n      }).then(res => {\n        this.creatorList = res.data?.list\n        this.total = res.data?.total\n      })\n    },\n    cancelConcern(creatorId) {\n      this.$request.delete(\"/concern/delete/\" + this.user.id + \"/\" + creatorId).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"取消关注成功\")\n          this.load()\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum)\n    },\n    homePage(userId){\n      this.$router.push('/front/HomePage?userId=' + userId)\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":";AA+BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACA;IACAD,KAAA;MACA,KAAAE,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAR,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAQ,MAAA,OAAAf,IAAA,CAAAgB;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA,KAAAb,WAAA,GAAAa,GAAA,CAAAnB,IAAA,EAAAoB,IAAA;QACA,KAAAX,KAAA,GAAAU,GAAA,CAAAnB,IAAA,EAAAS,KAAA;MACA;IACA;IACAY,cAAAC,SAAA;MACA,KAAAT,QAAA,CAAAU,MAAA,2BAAAtB,IAAA,CAAAgB,EAAA,SAAAK,SAAA,EAAAJ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAK,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAf,IAAA;QACA;UACA,KAAAc,QAAA,CAAAE,KAAA,CAAAR,GAAA,CAAAS,GAAA;QACA;MACA;IACA;IACAC,oBAAAtB,OAAA;MACA,KAAAI,IAAA,CAAAJ,OAAA;IACA;IACAuB,SAAAd,MAAA;MACA,KAAAe,OAAA,CAAAC,IAAA,6BAAAhB,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}