{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport E from \"wangeditor\";\nimport hljs from 'highlight.js';\nexport default {\n  name: \"EditHomePage\",\n  data() {\n    return {\n      form: {},\n      editor: null,\n      rules: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      blogId: this.$route.query.blogId\n    };\n  },\n  mounted() {\n    this.$request.get('/homePage/select/' + this.user.id).then(res => {\n      this.form = res.data || {};\n      //异步操作，等富文本页面渲染完再赋值\n      setTimeout(() => {\n        this.editor.txt.html(this.form.synopsis);\n      }, 0);\n    });\n    this.setRichText();\n  },\n  methods: {\n    handleAdd() {\n      // 新增数据\n      this.form = {}; // 新增数据的时候清空数据\n      this.setRichText(); // 初始化富文本编辑器\n    },\n    handleEdit(row) {\n      // 编辑数据\n      this.form = JSON.parse(JSON.stringify(row)); // 给form对象赋值  注意要深拷贝数据\n      this.fromVisible = true; // 打开弹窗\n      this.setRichText(); // 初始化富文本编辑器\n      setTimeout(() => {\n        this.editor.txt.html(this.form.synopsis); // 将编辑器内容赋值给form对象\n      }, 0);\n    },\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.form.synopsis = this.editor.txt.html();\n          this.form.userId = this.user.id;\n          this.$request({\n            url: this.form.id ? '/homePage/update' : '/homePage/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('保存成功');\n              //返回\n              this.$router.push('/front/Person');\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n    handleCoverSuccess(res) {\n      this.form.cover = res.data;\n    },\n    setRichText() {\n      this.$nextTick(() => {\n        this.editor = new E(`#editor`); // 实例化\\初始化\n        this.editor.highlight = hljs; // 配置高亮\n        this.editor.config.uploadImgServer = this.$baseUrl + '/files/editor/upload'; // 上传图片的接口\n        this.editor.config.uploadFileName = 'file'; // 上传图片的参数名\n        this.editor.config.zIndex = 0;\n        this.editor.config.uploadImgHeaders = {\n          token: this.user.token // token\n        };\n        this.editor.config.uploadImgParams = {\n          type: 'img'\n        };\n        this.editor.create(); // 创建\n      });\n    }\n  }\n};","map":{"version":3,"names":["E","hljs","name","data","form","editor","rules","user","JSON","parse","localStorage","getItem","blogId","$route","query","mounted","$request","get","id","then","res","setTimeout","txt","html","synopsis","setRichText","methods","handleAdd","handleEdit","row","stringify","fromVisible","save","$refs","formRef","validate","valid","userId","url","method","code","$message","success","$router","push","error","msg","handleCoverSuccess","cover","$nextTick","highlight","config","uploadImgServer","$baseUrl","uploadFileName","zIndex","uploadImgHeaders","token","uploadImgParams","type","create"],"sources":["src/views/front/EditHomePage.vue"],"sourcesContent":["<template>\r\n  <div style=\"width: 50%; margin: 5px auto\">\r\n    <div class=\"card\">\r\n      <div style=\"font-weight: bold;font-size: 24px;margin-bottom: 20px; text-align: center\">编辑主页</div>\r\n      <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item label=\"封面\" prop=\"cover\">\r\n          <el-upload\r\n              :action=\"$baseUrl + '/files/upload'\"\r\n              :headers=\"{ token: user.token }\"\r\n              list-type=\"picture\"\r\n              :on-success=\"handleCoverSuccess\"\r\n          >\r\n            <el-button type=\"primary\">上传封面</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item label=\"主页简介\" prop=\"synopsis\">\r\n          <div id=\"editor\"></div>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\" style=\"text-align: center\">\r\n        <el-button type=\"primary\" @click=\"save\" style=\"width: 200px\">保 存</el-button>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport E from \"wangeditor\"\r\nimport hljs from 'highlight.js'\r\n\r\nexport default {\r\n  name: \"EditHomePage\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      editor: null,\r\n      rules: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      blogId: this.$route.query.blogId\r\n    }\r\n  },\r\n  mounted() {\r\n    this.$request.get('/homePage/select/' + this.user.id).then(res => {\r\n      this.form = res.data || {}\r\n        //异步操作，等富文本页面渲染完再赋值\r\n        setTimeout(() => {\r\n          this.editor.txt.html(this.form.synopsis)\r\n        }, 0)\r\n    })\r\n    this.setRichText()\r\n  },\r\n  methods: {\r\n    handleAdd() {   // 新增数据\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.setRichText()  // 初始化富文本编辑器\r\n    },\r\n    handleEdit(row) {   // 编辑数据\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n      this.setRichText()  // 初始化富文本编辑器\r\n      setTimeout(() => {\r\n        this.editor.txt.html(this.form.synopsis) // 将编辑器内容赋值给form对象\r\n      }, 0)\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.form.synopsis = this.editor.txt.html()\r\n          this.form.userId = this.user.id\r\n          this.$request({\r\n            url: this.form.id ? '/homePage/update' : '/homePage/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              //返回\r\n              this.$router.push('/front/Person')\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleCoverSuccess(res) {\r\n      this.form.cover = res.data\r\n    },\r\n    setRichText() {\r\n      this.$nextTick(() => {\r\n        this.editor = new E(`#editor`)  // 实例化\\初始化\r\n        this.editor.highlight = hljs    // 配置高亮\r\n        this.editor.config.uploadImgServer = this.$baseUrl + '/files/editor/upload' // 上传图片的接口\r\n        this.editor.config.uploadFileName = 'file'  // 上传图片的参数名\r\n        this.editor.config.zIndex = 0\r\n        this.editor.config.uploadImgHeaders = {\r\n          token: this.user.token  // token\r\n        }\r\n        this.editor.config.uploadImgParams = {\r\n          type: 'img',\r\n        }\r\n        this.editor.create()  // 创建\r\n      })\r\n    },\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA2BA,OAAAA,CAAA;AACA,OAAAC,IAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;MACAC,KAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,MAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF;IACA;EACA;EACAG,QAAA;IACA,KAAAC,QAAA,CAAAC,GAAA,4BAAAV,IAAA,CAAAW,EAAA,EAAAC,IAAA,CAAAC,GAAA;MACA,KAAAhB,IAAA,GAAAgB,GAAA,CAAAjB,IAAA;MACA;MACAkB,UAAA;QACA,KAAAhB,MAAA,CAAAiB,GAAA,CAAAC,IAAA,MAAAnB,IAAA,CAAAoB,QAAA;MACA;IACA;IACA,KAAAC,WAAA;EACA;EACAC,OAAA;IACAC,UAAA;MAAA;MACA,KAAAvB,IAAA;MACA,KAAAqB,WAAA;IACA;IACAG,WAAAC,GAAA;MAAA;MACA,KAAAzB,IAAA,GAAAI,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAsB,SAAA,CAAAD,GAAA;MACA,KAAAE,WAAA;MACA,KAAAN,WAAA;MACAJ,UAAA;QACA,KAAAhB,MAAA,CAAAiB,GAAA,CAAAC,IAAA,MAAAnB,IAAA,CAAAoB,QAAA;MACA;IACA;IACAQ,KAAA;MAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAhC,IAAA,CAAAoB,QAAA,QAAAnB,MAAA,CAAAiB,GAAA,CAAAC,IAAA;UACA,KAAAnB,IAAA,CAAAiC,MAAA,QAAA9B,IAAA,CAAAW,EAAA;UACA,KAAAF,QAAA;YACAsB,GAAA,OAAAlC,IAAA,CAAAc,EAAA;YACAqB,MAAA,OAAAnC,IAAA,CAAAc,EAAA;YACAf,IAAA,OAAAC;UACA,GAA